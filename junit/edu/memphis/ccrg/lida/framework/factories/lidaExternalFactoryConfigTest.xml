<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<lidaFactoryConfigFile
	xmlns="http://ccrg.cs.memphis.edu/LidaFactoryConfigFileXMLSchema"
	xmlns:f="http://ccrg.cs.memphis.edu/LidaFactoriesXMLSchema"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

	<factoryConfig type="local">
		<f:factoryObjects>
			<f:factoryObject name="defaultExciteStrategy" default="true">
				<f:type>edu.memphis.ccrg.lida.framework.strategies.ExciteStrategy
					</f:type>
				<f:impl>edu.memphis.ccrg.lida.framework.strategies.LinearExciteStrategy
					</f:impl>
				<f:params>
					<f:param name="m" type="double">1.0</f:param>
					<f:param name="flyweight" type="boolean">true</f:param>
				</f:params>
			</f:factoryObject>
			<f:factoryObject name="defaultDecayStrategy" default="true">
				<f:type>edu.memphis.ccrg.lida.framework.strategies.DecayStrategy
					</f:type>
				<f:impl>edu.memphis.ccrg.lida.framework.strategies.LinearDecayStrategy
					</f:impl>
				<f:params>
					<f:param name="m" type="double">0.1</f:param>
					<f:param name="flyweight" type="boolean">true</f:param>
				</f:params>
			</f:factoryObject>
			<f:factoryObject name="slowExciteStrategy">
				<f:type>edu.memphis.ccrg.lida.framework.strategies.ExciteStrategy
					</f:type>
				<f:impl>edu.memphis.ccrg.lida.framework.strategies.LinearExciteStrategy
					</f:impl>
				<f:params>
					<f:param name="m" type="double">0.000000001</f:param>
					<f:param name="flyweight" type="boolean">true</f:param>
				</f:params>
			</f:factoryObject>
			<f:factoryObject name="slowDecayStrategy">
				<f:type>edu.memphis.ccrg.lida.framework.strategies.DecayStrategy
					</f:type>
				<f:impl>edu.memphis.ccrg.lida.framework.strategies.LinearDecayStrategy
					</f:impl>
				<f:params>
					<f:param name="m" type="double">0.000000001</f:param>
					<f:param name="flyweight" type="boolean">true</f:param>
				</f:params>
			</f:factoryObject>
			<f:factoryObject name="pamDefaultExciteStrategy">
				<f:type>edu.memphis.ccrg.lida.framework.strategies.ExciteStrategy
					</f:type>
				<f:impl>edu.memphis.ccrg.lida.framework.strategies.LinearExciteStrategy
					</f:impl>
				<f:params>
					<f:param name="m" type="double">1.0</f:param>
					<f:param name="flyweight" type="boolean">true</f:param>
				</f:params>
			</f:factoryObject>
			<f:factoryObject name="pamDefaultDecayStrategy">
				<f:type>edu.memphis.ccrg.lida.framework.strategies.DecayStrategy
					</f:type>
				<f:impl>edu.memphis.ccrg.lida.framework.strategies.LinearDecayStrategy
					</f:impl>
				<f:params>
					<f:param name="m" type="double">0.05</f:param>
					<f:param name="flyweight" type="boolean">true</f:param>
				</f:params>
			</f:factoryObject>
			<f:factoryObject name="noExciteStrategy">
				<f:type>edu.memphis.ccrg.lida.framework.strategies.ExciteStrategy
					</f:type>
				<f:impl>edu.memphis.ccrg.lida.framework.strategies.NoExciteStrategy
					</f:impl>
				<f:params>
					<f:param name="m" type="double">0.0</f:param>
					<f:param name="flyweight" type="boolean">true</f:param>
				</f:params>
			</f:factoryObject>
			<f:factoryObject name="noDecayStrategy">
				<f:type>edu.memphis.ccrg.lida.framework.strategies.DecayStrategy
					</f:type>
				<f:impl>edu.memphis.ccrg.lida.framework.strategies.NoDecayStrategy
					</f:impl>
				<f:params>
					<f:param name="m" type="double">0.0</f:param>
					<f:param name="flyweight" type="boolean">true</f:param>
				</f:params>
			</f:factoryObject>
		</f:factoryObjects>
	</factoryConfig>
</lidaFactoryConfigFile>