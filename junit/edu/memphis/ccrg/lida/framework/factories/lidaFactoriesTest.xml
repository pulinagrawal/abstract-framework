<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<!-- <lidaFactories xmlns="http://ccrg.cs.memphis.edu/LidaFactoriesXMLSchema"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://ccrg.cs.memphis.edu/LidaFactoriesXMLSchema LidaFactoriesXMLSchema.xsd">
 -->
 <lidaFactories>
 
	<factory name="StrategyFactory" depends="">
		<factoryType>edu.memphis.ccrg.lida.framework.factories.StrategyFactory
		</factoryType>
		<factoryImpl>edu.memphis.ccrg.lida.framework.factories.DefaultStrategyFactory
		</factoryImpl>
		<factoryConfig type="local">
			<factoryObjects>
				<factoryObject name="defaultExcite">
					<type>edu.memphis.ccrg.lida.framework.strategies.ExciteStrategy
					</type>
					<impl>edu.memphis.ccrg.lida.framework.strategies.LinearExciteStrategy
					</impl>
					<params>
						<param name="m" type="double">1.0</param>
						<param name="flyweight" type="boolean">true</param>
					</params>
				</factoryObject>
				<factoryObject name="defaultDecay">
					<type>edu.memphis.ccrg.lida.framework.strategies.DecayStrategy
					</type>
					<impl>edu.memphis.ccrg.lida.framework.strategies.LinearDecayStrategy
					</impl>
					<params>
						<param name="m" type="double">0.1</param>
						<param name="flyweight" type="boolean">true</param>
					</params>
				</factoryObject>
				<factoryObject name="slowExcite">
					<type>edu.memphis.ccrg.lida.framework.strategies.ExciteStrategy
					</type>
					<impl>edu.memphis.ccrg.lida.framework.strategies.LinearExciteStrategy
					</impl>
					<params>
						<param name="m" type="double">0.000000001</param>
						<param name="flyweight" type="boolean">true</param>
					</params>
				</factoryObject>
				<factoryObject name="slowDecay">
					<type>edu.memphis.ccrg.lida.framework.strategies.DecayStrategy
					</type>
					<impl>edu.memphis.ccrg.lida.framework.strategies.LinearDecayStrategy
					</impl>
					<params>
						<param name="m" type="double">0.000000001</param>
						<param name="flyweight" type="boolean">true</param>
					</params>
				</factoryObject>
				<factoryObject name="pamDefaultExcite">
					<type>edu.memphis.ccrg.lida.framework.strategies.ExciteStrategy
					</type>
					<impl>edu.memphis.ccrg.lida.framework.strategies.LinearExciteStrategy
					</impl>
					<params>
						<param name="m" type="double">1.0</param>
						<param name="flyweight" type="boolean">true</param>
					</params>
				</factoryObject>
				<factoryObject name="pamDefaultDecay">
					<type>edu.memphis.ccrg.lida.framework.strategies.DecayStrategy
					</type>
					<impl>edu.memphis.ccrg.lida.framework.strategies.LinearDecayStrategy
					</impl>
					<params>
						<param name="m" type="double">0.05</param>
						<param name="flyweight" type="boolean">true</param>
					</params>
				</factoryObject>
				<factoryObject name="noExcite">
					<type>edu.memphis.ccrg.lida.framework.strategies.ExciteStrategy
					</type>
					<impl>edu.memphis.ccrg.lida.framework.strategies.NoExciteStrategy
					</impl>
					<params>
						<param name="m" type="double">0.0</param>
						<param name="flyweight" type="boolean">true</param>
					</params>
				</factoryObject>
				<factoryObject name="noDecay">
					<type>edu.memphis.ccrg.lida.framework.strategies.DecayStrategy
					</type>
					<impl>edu.memphis.ccrg.lida.framework.strategies.NoDecayStrategy
					</impl>
					<params>
						<param name="m" type="double">0.0</param>
						<param name="flyweight" type="boolean">true</param>
					</params>
				</factoryObject>
			</factoryObjects>
		</factoryConfig>
	</factory>

	<factory name="FrameworkTaskFactory" depends="StrategyFactory">
		<factoryType>edu.memphis.ccrg.lida.framework.factories.FrameworkTaskFactory
		</factoryType>
		<factoryImpl>edu.memphis.ccrg.lida.framework.factories.DefaultFrameworkTaskFactory
		</factoryImpl>
		<factoryConfig type="local">
			<factoryObjects>
				<factoryObject name="BasicAttentionCodelet">
					<type>edu.memphis.ccrg.lida.attentioncodelets.AttentionCodelet
					</type>
					<impl>edu.memphis.ccrg.lida.attentioncodelets.BasicAttentionCodelet
					</impl>
					<params>
						<param name="refractoryPeriod" type="int">50</param>
						<param name="coalitionDecayStrategy" type="string">coalitionDecay
						</param>
						<param name="baseLevelDecayStrategy" type="string">slowDecay
						</param>
						<param name="baseLevelExciteStrategy" type="string">slowExcite
						</param>
						<param name="baseLevelRemovalThreshold" type="double">0.0
						</param>
						<param name="baseLevelActivation" type="double">1.0</param>
						<param name="totalActivationStrategy" type="string">DefaultTotalActivationStrategy
						</param>
					</params>
				</factoryObject>
				<factoryObject name="SensoryMemoryBackgroundTask">
					<type>edu.memphis.ccrg.lida.framework.tasks.FrameworkTask
					</type>
					<impl>edu.memphis.ccrg.lida.sensorymemory.SensoryMemoryBackgroundTask
					</impl>
					<params>
						<param name="baseLevelDecayStrategy" type="string">slowDecay
						</param>
						<param name="baseLevelExciteStrategy" type="string">slowExcite
						</param>
						<param name="baseLevelRemovalThreshold" type="double">0.0
						</param>
						<param name="baseLevelActivation" type="double">0.1</param>
						<param name="totalActivationStrategy" type="string">DefaultTotalActivationStrategy
						</param>
					</params>
				</factoryObject>
			</factoryObjects>
		</factoryConfig>
	</factory>

</lidaFactories>